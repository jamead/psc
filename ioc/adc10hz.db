############# 10Hz Status PV's (MSG ID 30) #########################


record(longin, "$(P){PSC:$(NO)}CNT-I") {
    field(DESC, "10Hz Counter")
    field(DTYP, "PSC Reg")
    field(INP , "@Rx$(NO) 31 0")
    field(SCAN,"I/O Intr")
}

