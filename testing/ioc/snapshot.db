

record(waveform,"$(P){$(NO)}SS:Hdr-Wfm") {
    field(DTYP, "PSC Block I32 In")
    field(INP , "@Wfm$(NO) 51 0 160")
    field(SCAN,"I/O Intr")
    field(FTVL,"DOUBLE")
    field(NELM,"$(BUF_LEN)")
}

