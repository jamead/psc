
 

############# 10Hz Status PV's (MSG ID 30) #########################


record(longin, "$(P){$(NO)}LiveTime-I") {
    field(DESC, "LiveTime Counter (in sec)")
    field(DTYP, "PSC Reg")
    field(INP , "@Rx$(NO) 31 0")
    field(SCAN,"I/O Intr")
    field(EGU, "sec")
}

record(longin, "$(P){$(NO)}TS-NS-I") {
    field(DESC, "EVR Timestamp (ns)")
    field(DTYP, "PSC Reg")
    field(INP , "@Rx$(NO) 31 4")
    field(SCAN,"I/O Intr")
}

record(longin, "$(P){$(NO)}TS-S-I") {
    field(DESC, "EVR Timestamp (s)")
    field(DTYP, "PSC Reg")
    field(INP , "@Rx$(NO) 31 8")
    field(SCAN,"I/O Intr")
}






