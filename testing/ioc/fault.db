

record(waveform,"$(P){$(NO)}Chan$(CHAN):FLT:Hdr-Wfm") {
    field(DTYP, "PSC Block I32 In")
    field(INP , "@Wfm$(NO) $(MSGID) 0 40")
    field(SCAN,"I/O Intr")
    field(FTVL,"DOUBLE")
    field(NELM,"$(BUF_LEN)")
}

