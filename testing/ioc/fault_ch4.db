

record(waveform,"$(P){$(NO)}Chan4:FLT:Hdr-Wfm") {
    field(DTYP, "PSC Block I32 In")
    field(INP , "@Wfm$(NO) 55 0 160")
    field(SCAN,"I/O Intr")
    field(FTVL,"DOUBLE")
    field(NELM,"$(BUF_LEN)")
}

