record(mbbo, "$(P){$(NO)}Chan$(CHAN):DAC_OpMode-SP") {
    field(DESC, "Sets the DAC operating Mode")
    field(DTYP, "PSC Single U32")
    field(OUT , "@Tx$(NO) $(CHAN) 100")
    #field( PINI, "YES")
    field( ZRST, "SmoothRamp")
    field( ONST, "RampTable")
    field( TWST, "FOFB")
    field( THST, "Jump")
    field( ZRVL, "0")
    field( ONVL, "1")
    field( TWVL, "2")
    field( THVL, "3")
    #field( IVOA, "Don't drive outputs")
    info(autosaveFields, "VAL")
    field(FLNK, "$(P){$(NO)}GoRegSingleWrite$(CHAN)")
}

