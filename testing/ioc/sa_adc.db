



### Slow Acquisition (10Hz) ADC data

record(longin, "$(P){$(NO)}$(CHAN):DCCT1-I") {
    field(DESC, "DCCT 1 Readback")
    field(DTYP, "PSC Reg")
    field(INP , "@Rx$(NO) 31 12")
    field(SCAN,"I/O Intr")
    field(EGU,  "")
}

