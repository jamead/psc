record(longin, "$(P):EpochTime-I") {
    field(DESC, "Epoch time in seconds")
    field(VAL, "1712763054")  # Example epoch time
    field(SCAN, "Passive")
    field(FLNK, "$(P):EpochTime-STR")
}

record(stringin, "$(P):EpochTime-STR") {
    field(DESC, "Formatted timestamp")
    field(DTYP, "Soft Timestamp")
    field(INP , "@ %Y-%m-%d %H:%M:%S")  # Customize date/time format here
    field(TSEL, "$(P):EpochTime-I.VAL NPP NMS")
    field(TSE, "-2")
    field(SCAN, "Passive")
}




