record(longin, "$(P){$(PSC)}SS:Currbufaddr-I") {
    field(DESC, "Current DDR Address of Snapshot Pointer")
    field(DTYP, "PSC Reg")
    field(INP , "@Wfm$(PSC) 50 0")
    field(SCAN,"I/O Intr")
}

