############# 10Hz Status PV's (MSG ID 30) #########################


record(longin, "$(P){PSC:$(NO)}CNT-I") {
    field(DESC, "10Hz Counter")
    field(DTYP, "PSC Reg")
    field(INP , "@Rx$(NO) 31 0")
    field(SCAN,"I/O Intr")
}

record(longin, "$(P){PSC:$(NO)}TS-NS-I") {
    field(DESC, "EVR Timestamp (ns)")
    field(DTYP, "PSC Reg")
    field(INP , "@Rx$(NO) 31 4")
    field(SCAN,"I/O Intr")
}

record(longin, "$(P){PSC:$(NO)}TS-S-I") {
    field(DESC, "EVR Timestamp (s)")
    field(DTYP, "PSC Reg")
    field(INP , "@Rx$(NO) 31 8")
    field(SCAN,"I/O Intr")
}


